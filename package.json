{
	"name": "muvifind",
	"version": "1.0.0",
	"description": "A movie discovery app that lets you find movies by selecting your favorite actors",
	"private": true,
	"type": "module",
	"keywords": [
		"movie",
		"discovery",
		"actors",
		"tmdb",
		"react",
		"react-router",
		"cloudflare-workers",
		"tailwindcss"
	],
	"author": "berkziya",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/berkziya/muvifind.git"
	},
	"homepage": "https://muvifind.berkziya.workers.dev",
	"scripts": {
		"build": "react-router build",
		"cf-typegen": "wrangler types",
		"deploy": "pnpm run build && wrangler deploy",
		"dev": "react-router dev",
		"postinstall": "pnpm run cf-typegen",
		"preview": "pnpm run build && vite preview",
		"typecheck": "pnpm run cf-typegen && react-router typegen && tsc -b",
		"lint": "tsc --noEmit",
		"start": "pnpm run dev"
	},
	"dependencies": {
		"isbot": "^5.1.31",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-router": "^7.9.4"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.13.16",
		"@react-router/dev": "^7.9.4",
		"@tailwindcss/vite": "^4.1.16",
		"@types/node": "^24.9.1",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"tailwindcss": "^4.1.16",
		"typescript": "^5.9.3",
		"vite": "^7.1.12",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^4.45.1"
	}
}